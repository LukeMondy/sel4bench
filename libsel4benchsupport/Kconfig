menuconfig LIB_SEL4_BENCHSUPPORT
    bool "libsel4benchsupport"
    depends on HAVE_LIB_SEL4 && HAVE_LIBC && HAVE_LIB_UTILS && HAVE_LIB_SEL4_VKA \
    && HAVE_LIB_SEL4_VSPACE && HAVE_LIB_SEL4_ALLOCMAN
    default y
    help
        Support library for sel4bench apps.

config HAVE_LIB_SEL4_BENCHSUPPORT
    bool
